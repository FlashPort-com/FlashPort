{
  "name": "flashport",
  "version": "1.0.0",
  "description": "FlashPort",
  "author": "Kenny Lerma",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.min.mjs",
  "unpkg": "dist/index.umd.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "rollup": "^2.70.0",
    "terser": "^5.12.0",
    "ts-loader": "^9.2.7",
    "typescript": "^4.4.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -w",
    "webp": "webpack",
    "clean": "del /F /Q  dist",
    "build:prod": "npm run clean && tsc && npm run bundle:esm && npm run bundle:esm:min && npm run bundle:umd && npm run bundle:umd:min",
    "bundle:esm": "rollup dist/Test3D.js --file dist/index.mjs --format esm",
    "bundle:esm:min": "terser --ecma 6 --compress --mangle --module -o dist/index.min.mjs -- dist/index.mjs",
    "bundle:umd": "rollup dist/Test3D.js --file dist/index.umd.js --format umd --name Test3D",
    "bundle:umd:min": "terser --ecma 6 --compress --mangle -o dist/index.umd.min.js -- dist/index.umd.js"
  },
  "dependencies": {
    "@types/css-font-loading-module": "^0.0.7",
    "@types/npm": "^7.19.0",
    "typings": "^2.1.1"
  }
}
